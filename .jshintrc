{
    "strict" : true,  // "use strict" を強制
    "indent" : 4,    // インデント
    "maxlen" : 200,   // 一行の最大文字数
    "unused" : true, // 宣言のみで使用していない変数を検出

    // グローバル変数へのアクセスの管理
    "undef"     : true, // グローバル変数へのアクセスを禁止
    "browser"   : true,  // ブラウザ用のやつは許可
    "camelcase" : true, // キャメルケース
    "eqeqeq"    : true, // ==、!=の使用禁止
    "devel"     : true  // console、alertを許可
    "debug"     : true,  // debugger を許可

    "globals": {
        "_"      : false,
        "$"      : false,
        "$j"     : false,
        "jQuery" : false
    }
}
